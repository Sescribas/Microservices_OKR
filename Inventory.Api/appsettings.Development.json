    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "IdentityApi": {
    "Host": "https://localhost:7289",
    "Endpoints": {
      "Login": "/api/Auth/login",
      "Register": "/api/Auth/register",
      "GetAll": "/api/User/getall"
    }
  },
  "ProductApi": {
